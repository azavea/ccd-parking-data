#!/bin/bash

SEARCH_RADIUS=5

docker run -it -v "$(pwd)/data/interim/parking_data/":/data/ \
    --rm shst-image shst match /data/curbs_12-19_WGS.geojson \
    --search-radius=$SEARCH_RADIUS \
    --snap-side-of-street \
    --tile-hierarchy=8 \
    --best-direction \
    --out=/data/non_offset/NEW_40.geojson

